{
  "source": {
    "include": ["src"],
    "exclude": [
      "src/typedefs"
    ]
  },
  "opts": {
    "access": ["public", "undefined"],
    "destination": "docs",
    "readme": "README.md",
    "recurse": true,
    "template": "node_modules/jsdoc-template"
  },
  "plugins": [
    "plugins/markdown",
    "node_modules/jsdoc-tsimport-plugin",
    "node_modules/jsdoc-typeof-plugin"
  ],
  "templates": {
    "referenceTitle": "",
    "disableSort": false,
    "collapse": true,
    "default": {
      "includeDate": false
    }
  }
}
